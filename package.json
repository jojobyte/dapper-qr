{
  "name": "@jojobyte/dapper-qr",
  "version": "1.3.0",
  "description": "A simple (zero dependency) QR Code generator in Vanilla JavaScript",
  "author": "jojobyte <byte@jojo.io> (https://jojo.io/)",
  "contributors": [
    "papnkukn",
    "leMaik",
    "jp-ed",
    "adrien-powder",
    "jojobyte <byte@jojo.io> (https://jojo.io/)"
  ],
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/jojobyte"
    },
    "https://ko-fi.com/jojobyte",
    "https://liberapay.com/jojobyte/"
  ],
  "license": "MIT",
  "main": "lib/qrcode.js",
  "scripts": {
    "banner": "node -p -e 'let b,r=require,p=r(`./package.json`);b=`/*! ${p.name} v${p.version} | ${p.homepage} | MIT license */\n`;r(`fs`).writeFileSync(`dist/qrcode.min.js`,b)'",
    "minify": "npm run banner && npx -y minify@9 lib/qrcode.js >> dist/qrcode.min.js",
    "test": "npx -y nodeunit@0.11.3 test"
  },
  "keywords": [
    "qr",
    "code",
    "qrcode",
    "svg",
    "zero-dependency",
    "zero",
    "dependency"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jojobyte/dapper-qr.git"
  },
  "bugs": {
    "url": "https://github.com/jojobyte/dapper-qr/issues"
  },
  "homepage": "https://github.com/jojobyte/dapper-qr"
}
